<%= form_with model: @lab_report, url: lab_reports_path do |form| %>
  <%= form.collection_select :user_id, User.all, :id, ->(user) { "#{user.first_name} #{user.last_name}" }, { :include_blank => true } %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :description %>
  <%= form.text_area :description %>
  <%= form.label :grade %>
  <%= form.select :grade, %w(A B C D E FX F) %>
  <%= form.submit %>
<% end %>
